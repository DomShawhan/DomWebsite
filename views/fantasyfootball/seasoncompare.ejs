<%- include("./header") %>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th scope="col">Rank</th>
            <th scope="col">Team</th>
            <th scope="col">W</th>
            <th scope="col">L</th>
            <th scope="col">T</th>
            <th scope="col">Total Points</th>
            <th scope="col">PPG</th>
            <th scope="col">Min</th>
            <th scope="col">Max</th>
            <th scope="col">Std Dev</th>
            <th scope="col">Adjusted Average</th>
        </tr>
    </thead>
    <tbody>
        <% data.forEach(team=> { %>
            <tr>
                <td class="rank">
                    <%= team.rank != 0 ? team.rank : '' %>
                </td>
                <td scope="row">
                    <% if(team.logo_url.length> 0) { %>
                        <img height="25px" width="25px" src="<%= team.logo_url %>">
                    <% } %>
                    <%= team.name %>
                </td>
                <td class="wins <%= team.wins > 0 ? 'green' : 'red' %>">
                    <%= team.wins %>
                </td>
                <td class="losses <%= team.losses > 0 ? 'green' : 'red' %>">
                    <%= team.losses %>
                </td>
                <td class="ties <%= team.ties > 0 ? 'green' : 'red' %>">
                    <%= team.ties %>
                </td>
                <td class="total <%= team.seasonTotal > 0 ? 'green' : 'red' %>">
                    <%= team.seasonTotal.toFixed(2) %>
                </td>
                <td class="avg <%= team.avg > 0 ? 'green' : 'red' %>">
                    <%= team.avg.toFixed(2) %>
                </td>
                <td class="min <%= team.min > 0 ? 'green' : 'red' %>">
                    <%= team.min.toFixed(2) %>
                </td>
                <td class="max <%= team.max > 0 ? 'green' : 'red' %>">
                    <%= team.max.toFixed(2) %>
                </td>
                <td class="stddev <%= team.stddev < 0 ? 'green' : 'red' %>">
                    <%= team.stddev.toFixed(2) %>
                </td>
                <td class="adjavg <%= team.avgMinusTopAndBottom25Percent > 0 ? 'green' : 'red' %>">
                    <%= team.avgMinusTopAndBottom25Percent.toFixed(2) %>
                </td>
            </tr>
            <% }); %>
    </tbody>
</table>
<%- include("./footer") %>