<%- include("./header") %>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th></th>
            <th scope="col">Team</th>
        </tr>
    </thead>
    <tbody>
        <% teams.forEach(team=> { %>
            <tr>
                <td></td>
                <td scope="row">
                    <% if(team.logo_url.length> 0) { %>
                        <img height="25px" width="25px" src="<%= team.logo_url %>">
                    <% } %>
                    <%= team.name %>
                </td>
            </tr>
            <tr>
                <td></td>
                <td>
                    <table class="table table-striped table-hover">
                        <thead>
                            <tr>
                                <th scope="col"></th>
                                <th scope="col">Position</th>
                                <th scope="col">Total</th>
                                <th scope="col">PPG</th>
                                <th scope="col">Min</th>
                                <th scope="col">Max</th>
                                <th scope="col">Std Dev</th>
                                <th scope="col">Adjusted Average</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td scope="row"></td>
                                <td>QB</td>
                                <td class="<%= team.qbTotal > 0 ? 'green' : 'red' %>"><%= team.qbTotal.toFixed(2) %></td>
                                <td class="<%= team.avg.qb > 0 ? 'green' : 'red' %>"><%= team.avg.qb.toFixed(2) %></td>
                                <td class="<%= team.min.qb > 0 ? 'green' : 'red' %>"><%= team.min.qb.toFixed(2) %></td>
                                <td class="<%= team.max.qb > 0 ? 'green' : 'red' %>"><%= team.max.qb.toFixed(2) %></td>
                                <td class="<%= team.stddev.qb < 0 ? 'green' : 'red' %>"><%= team.stddev.qb.toFixed(2) %></td>
                                <td class="<%= team.avgMinusTopAndBottom25Percent.qb > 0 ? 'green' : 'red' %>"><%= team.avgMinusTopAndBottom25Percent.qb.toFixed(2) %></td>
                            </tr>
                            <tr>
                                <td scope="row"></td>
                                <td>RB</td>
                                <td class="<%= team.rbTotal > 0 ? 'green' : 'red' %>"><%= team.rbTotal.toFixed(2) %></td>
                                <td class="<%= team.avg.rb > 0 ? 'green' : 'red' %>"><%= team.avg.rb.toFixed(2) %></td>
                                <td class="<%= team.min.rb > 0 ? 'green' : 'red' %>"><%= team.min.rb.toFixed(2) %></td>
                                <td class="<%= team.max.rb > 0 ? 'green' : 'red' %>"><%= team.max.rb.toFixed(2) %></td>
                                <td class="<%= team.stddev.rb < 0 ? 'green' : 'red' %>"><%= team.stddev.rb.toFixed(2) %></td>
                                <td class="<%= team.avgMinusTopAndBottom25Percent.rb > 0 ? 'green' : 'red' %>"><%= team.avgMinusTopAndBottom25Percent.rb.toFixed(2) %></td>
                            </tr>
                            <tr>
                                <td scope="row"></td>
                                <td>WR</td>
                                <td class="<%= team.wrTotal > 0 ? 'green' : 'red' %>"><%= team.wrTotal.toFixed(2) %></td>
                                <td class="<%= team.avg.wr > 0 ? 'green' : 'red' %>"><%= team.avg.wr.toFixed(2) %></td>
                                <td class="<%= team.min.wr > 0 ? 'green' : 'red' %>"><%= team.min.wr.toFixed(2) %></td>
                                <td class="<%= team.max.wr > 0 ? 'green' : 'red' %>"><%= team.max.wr.toFixed(2) %></td>
                                <td class="<%= team.stddev.wr < 0 ? 'green' : 'red' %>"><%= team.stddev.wr.toFixed(2) %></td>
                                <td class="<%= team.avgMinusTopAndBottom25Percent.wr > 0 ? 'green' : 'red' %>"><%= team.avgMinusTopAndBottom25Percent.wr.toFixed(2) %></td>
                            </tr>
                            <tr>
                                <td scope="row"></td>
                                <td>TE</td>
                                <td class="<%= team.teTotal > 0 ? 'green' : 'red' %>"><%= team.teTotal.toFixed(2) %></td>
                                <td class="<%= team.avg.te > 0 ? 'green' : 'red' %>"><%= team.avg.te.toFixed(2) %></td>
                                <td class="<%= team.min.te > 0 ? 'green' : 'red' %>"><%= team.min.te.toFixed(2) %></td>
                                <td class="<%= team.max.te > 0 ? 'green' : 'red' %>"><%= team.max.te.toFixed(2) %></td>
                                <td class="<%= team.stddev.te < 0 ? 'green' : 'red' %>"><%= team.stddev.te.toFixed(2) %></td>
                                <td class="<%= team.avgMinusTopAndBottom25Percent.te > 0 ? 'green' : 'red' %>"><%= team.avgMinusTopAndBottom25Percent.te.toFixed(2) %></td>
                            </tr>
                            <tr>
                                <td scope="row"></td>
                                <td>K</td>
                                <td class="<%= team.kTotal > 0 ? 'green' : 'red' %>"><%= team.kTotal.toFixed(2) %></td>
                                <td class="<%= team.avg.k > 0 ? 'green' : 'red' %>"><%= team.avg.k.toFixed(2) %></td>
                                <td class="<%= team.min.k > 0 ? 'green' : 'red' %>"><%= team.min.k.toFixed(2) %></td>
                                <td class="<%= team.max.k > 0 ? 'green' : 'red' %>"><%= team.max.k.toFixed(2) %></td>
                                <td class="<%= team.stddev.k < 0 ? 'green' : 'red' %>"><%= team.stddev.k.toFixed(2) %></td>
                                <td class="<%= team.avgMinusTopAndBottom25Percent.k > 0 ? 'green' : 'red' %>"><%= team.avgMinusTopAndBottom25Percent.k.toFixed(2) %></td>
                            </tr>
                            <tr>
                                <td scope="row"></td>
                                <td>DEF</td>
                                <td class="<%= team.dTotal > 0 ? 'green' : 'red' %>"><%= team.dTotal.toFixed(2) %></td>
                                <td class="<%= team.avg.d > 0 ? 'green' : 'red' %>"><%= team.avg.d.toFixed(2) %></td>
                                <td class="<%= team.min.d > 0 ? 'green' : 'red' %>"><%= team.min.d.toFixed(2) %></td>
                                <td class="<%= team.max.d > 0 ? 'green' : 'red' %>"><%= team.max.d.toFixed(2) %></td>
                                <td class="<%= team.stddev.d < 0 ? 'green' : 'red' %>"><%= team.stddev.d.toFixed(2) %></td>
                                <td class="<%= team.avgMinusTopAndBottom25Percent.d > 0 ? 'green' : 'red' %>"><%= team.avgMinusTopAndBottom25Percent.d.toFixed(2) %></td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
        <% }); %>
    </tbody>
</table>

<%- include("./footer") %>